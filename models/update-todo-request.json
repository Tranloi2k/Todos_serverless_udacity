{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "UpdateToDoRequest",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "pattern": "^(?!\\s*$).+",
      "minLength": 1
    },
    "dueDate": {
      "type": "string"
        },
    "done": {
      "type": "boolean"
    },
    "status": {
      "type": "string"
    }
  },
  "required": ["name", "dueDate", "done", "status"],
  "additionalProperties": false
}
